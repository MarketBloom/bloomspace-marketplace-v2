# Place your StackBlitz environment variables here,
# and they will be securely synced to your account.

# Environment variables
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key
VITE_SENTRY_DSN=your_sentry_dsn

# Application Configuration
VITE_APP_NAME=BloomSpace
VITE_APP_URL=http://localhost:8080
VITE_API_URL=http://localhost:8080/api

# Google Maps Configuration
VITE_GOOGLE_MAPS_LIBRARIES=places,geocoding,distance-matrix
VITE_GOOGLE_MAPS_REGION=AU
VITE_GOOGLE_MAPS_LANGUAGE=en

# Development settings
VITE_ENABLE_MOCK_API=false
VITE_DEBUG=true
